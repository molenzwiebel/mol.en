class Bool {
    def __or(other: Bool) -> Bool {
        if (this) {
            return true
        } elseif (other) {
            return true
        } else {
            return false
        }
    }

    def __and(other: Bool) -> Bool {
        if (this) {
            if (other) return true
            else return false
        } else {
            return false
        }
    }
}
